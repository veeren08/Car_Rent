<center>
  <div id="formContent" style="max-width:700px;"><br>
    <div class="row">
      <div class="col-md-10 mx-auto">
        <%#= form_with model: [@account, @vehicle] do |f| %>
        <%= form_for (@vehicle), url: user_vehicles_path(vehicle_id: @vehicle.id) do |f| %>
        <%#= form_with model: @vehicle do |f| %>
          <div class="form-group row">
            <div class="col-sm-6">
              <%= f.label :license %><br>
              <%= f.text_field :license, class:"form-control" %>
            </div>
            <div class="col-sm-6">
              <%= f.label :PlateNumber %><br>
              <%= f.text_field :PlateNumber, class:"form-control" %>
            </div>
          </div>
          <div class="form-group row">
            <div class="col-sm-6">
              <%= f.label :manufacturer %><br>
              <%= f.text_field :manufacturer, class:"form-control" %>
            </div>
            <div class="col-sm-6">
              <%= f.label :model %><br>
              <%= f.text_field :model, class:"form-control" %>
            </div>
          </div>
          <div class="form-group row">
            <div class="col-sm-6">
              <%= f.label :hourlyRentalRate %><br>
              <%= f.number_field :hourlyRentalRate, class:"form-control" %>
            </div>
          <div class="col-sm-3">
            <%= f.label :style %><br>
            <%= f.text_field :style, class:"form-control" %>
          </div>
          <div class="col-sm-3">
            <%= f.label :city %><br>
            <%= f.text_field :city, class:"form-control" %>
          </div>
        </div>
        <div class="form-group row">
          <div class="col-sm-6">
            <%= f.label :state %><br>
            <%= f.text_field :state, class:"form-control" %>
          </div>
          <div class="col-sm-6">
            <%= f.label :country %><br />
            <%= f.text_field :country, class:"form-control" %>
          </div>
        </div>
        <div class="form-group row">
          <div class="col-sm-6">
            <%= f.label :mobile %><br>
            <%= f.text_field :mobile, class:"form-control" %>
          </div>
          <div class="col-sm-6">
            <%= f.label :status %><br>
            <%= f.text_field :status, class:"form-control" %>
          </div>
        </div>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div> 
</center>